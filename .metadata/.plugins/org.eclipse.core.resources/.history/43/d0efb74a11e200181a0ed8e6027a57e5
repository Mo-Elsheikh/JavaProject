
public class HomeWorke {
	  
      //  recursive method
	   public static int Fib( int n) { 
	       if (n == 1) { 
	          return 0;
	       }
	       else if (n == 2) {

	           return 1;
	       }
	       int f1 = 0;

	       int f2 = 1;

	       int fib = 0;

	       for(int i=3; i<=n; i++){

	           fib = f1 + f2;

	           f1 = f2;

	           f2 = fib;

	       }

	      

	       return fib;

	   }

}
